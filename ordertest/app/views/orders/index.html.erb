<h1>Orders</h1>

<table>
 <thead>
   <tr>
     <th>ID</th>
     <th>Delivery Address</th>
     <th></th>
   </tr>
 </thead>
 <tbody>
   <% @orders.each do |order| %>
   <tr>
     <td><%= order.id %></td>
     <td><%= order.delivery_address %></td>
     <th><%= link_to "View", order_path(order)%></th>
   </tr>
   <% end %>   
 </tbody>
</table>

<h2>import orders</h2>
<%= form_tag import_orders_path, multipart: true do %>
<%= file_field_tag :file %>
<%= submit_tag "import" %>
<% end %>